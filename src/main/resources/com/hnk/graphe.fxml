<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/17"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.hnk.GrapheController"
            stylesheets="style.css"
            prefWidth="650"
            prefHeight="350"
            styleClass="graphe-container">

    <AreaChart fx:id="rdvChart"
               layoutX="40"
               layoutY="30"
               prefWidth="550"
               prefHeight="350"
               animated="true">

        <xAxis>
            <CategoryAxis label="Mois"/>
        </xAxis>

        <yAxis>
            <NumberAxis fx:id="yAxis"
                        label="Nombre de RDV"/>
        </yAxis>

    </AreaChart>


</AnchorPane>
